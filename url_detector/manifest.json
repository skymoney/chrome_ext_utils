{
  "manifest_version": 2,

  "name": "Url Detector",
  "description": "simple url detector for jump or click",
  "version": "1.0",

  "background": {"scripts": ["url_detect.js"]},

  "content_scripts": [
    {
      "matches": ["http://*/*", "file://*/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "tabs"
  ]

}
